{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "old_price": {
      "type": "decimal"
    },
    "cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cart.cart",
      "inversedBy": "products"
    },
    "product_url": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string"
    },
    "payment_info": {
      "displayName": "payment_info",
      "type": "component",
      "repeatable": false,
      "component": "payment-info.payment-info"
    }
  }
}
